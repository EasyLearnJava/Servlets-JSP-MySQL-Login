<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	
	<display-name>Servlet-HTML-HashMap-Login</display-name>

	<welcome-file-list>
		<welcome-file>login.html</welcome-file>
	</welcome-file-list>

	<context-param>
		<param-name>instructor</param-name>
		<param-value>Raghu</param-value>
	</context-param>

	<servlet>
		<servlet-name>loginServletCall</servlet-name>
		<servlet-class>com.easylearnjava.web.LoginServlet</servlet-class>
		<init-param>
			<param-name>project</param-name>
			<param-value>Simple Servlets HTML Login project</param-value>
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>loginServletCall</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

</web-app>